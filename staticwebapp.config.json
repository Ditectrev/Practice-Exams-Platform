{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "HEAD", "PATCH"],
      "rewrite": "/api/{*restOfPath}"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/css/*",
      "/static/js/*",
      "/static/media/*",
      "*.{png,jpg,gif,svg,ico,woff,woff2,ttf,eot}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}
